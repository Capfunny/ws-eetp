				<xs:element name="Account" type="tns:AccountType">
					<xs:annotation>
						<xs:documentation>Счёт участника закупки, на котором будут заблокированы средства</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="AmountToHoldx100" type="xs:integer">
					<xs:annotation>
						<xs:documentation>Сумма, которую необходимо заблокировать (умноженная на 100)</xs:documentation>
					</xs:annotation>
