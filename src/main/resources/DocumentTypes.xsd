		<xs:annotation>
			<xs:documentation>Банковский идентификационный код банка</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="9"/>
			<xs:pattern value="[0-9]{9}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="string-30">
		<xs:annotation>
			<xs:documentation>Произвольный текст длиной от 1 до 30 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OperatorNameType">
		<xs:annotation>
			<xs:documentation>Код площадки - оператора электронной закупки</xs:documentation>
		</xs:annotation>
		<xs:restriction base="tns:string-30"/>
